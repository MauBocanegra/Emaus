<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/item_movimiento_label_titulo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Retiro en efectivo"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="@color/textColorPrimary"
        android:textAllCaps="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/item_movimiento_label_fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp"
        android:text="10/Agosto/2018 - 17:50"
        android:textColor="@color/textColorPrimary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item_movimiento_label_titulo" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha | "
        app:layout_constraintBottom_toBottomOf="@+id/item_movimiento_label_fecha"
        app:layout_constraintEnd_toStartOf="@+id/item_movimiento_label_fecha"
        app:layout_constraintTop_toTopOf="@+id/item_movimiento_label_fecha" />

    <TextView
        android:id="@+id/item_movimiento_label_cantidad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp"
        android:text="$4,000"
        android:textColor="@color/textColorPrimary"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item_movimiento_label_fecha" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cantidad | "
        app:layout_constraintBottom_toBottomOf="@+id/item_movimiento_label_cantidad"
        app:layout_constraintEnd_toStartOf="@+id/item_movimiento_label_cantidad"
        app:layout_constraintTop_toTopOf="@+id/item_movimiento_label_cantidad" />

    <TextView
        android:id="@+id/item_movimiento_label_referencia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="8dp"
        android:text="0143006540003450"
        android:textColor="@color/textColorPrimary"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/item_movimiento_label_cantidad" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:text="Referencia | "
        app:layout_constraintBottom_toBottomOf="@+id/item_movimiento_label_referencia"
        app:layout_constraintEnd_toStartOf="@+id/item_movimiento_label_referencia"
        app:layout_constraintTop_toTopOf="@+id/item_movimiento_label_referencia" />
</android.support.constraint.ConstraintLayout>